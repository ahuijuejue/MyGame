local RecieveRunMsgResponse = class("RecieveRunMsgResponse")

function RecieveRunMsgResponse:ctor()
	--响应消息号
	self.order = 11007
	--返回结果,1 成功
	self.result =  ""
	--滚动条内容
	self.param1 =  ""	
end

function RecieveRunMsgResponse:RecieveRunMsgResponse(data)
	if data.result == 1 then
		NoticeData:insertNotice(data.param1)
		showNotice()
	end
end

return RecieveRunMsgResponse