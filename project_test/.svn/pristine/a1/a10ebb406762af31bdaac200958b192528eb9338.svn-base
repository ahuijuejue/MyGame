--[[
进入公会商店
]]
local ShopBaseVisitor = import(".ShopBaseVisitor")
local ShopUnionVisitor = class("ShopUnionVisitor", ShopBaseVisitor)

function ShopUnionVisitor:getNetOrder()
	return "OpenShopUnion"
end

function ShopUnionVisitor:getShopSceneName()
	return "ShopUnionScene"
end

function ShopUnionVisitor:getShopType()
	return 9
end

function ShopUnionVisitor:isShopOpen()
	return UserData.unionId ~= ""
end

return ShopUnionVisitor
