local SlotModel = class("SlotModel")

function SlotModel:ctor()
	self.pos = 0
	self.times = 0
	self.itemsData = {}
	self.price1 = 200
	self.price2 = 500
	self.gold = 10000
	self.scale = {0.1,0.3,0.5,0.7,0.9}
end

function SlotModel:update(data)
	self.times = data.param2
	for i,v in ipairs(data.a_param1) do
		self.itemsData[i] = v
	end
end

return SlotModel