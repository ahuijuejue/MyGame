--[[
进入普通商店
]]
local ShopBaseVisitor = import(".ShopBaseVisitor")
local ShopNormalVisitor = class("ShopNormalVisitor", ShopBaseVisitor)

function ShopNormalVisitor:getNetOrder()
	return "OpenShopNormal"
end 

function ShopNormalVisitor:getShopSceneName()
	return "ShopNormalScene"
end 

function ShopNormalVisitor:getShopType()
	return 1
end 

function ShopNormalVisitor:isShopOpen()
	return true
end 

return ShopNormalVisitor