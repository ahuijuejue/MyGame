local OpenOnearmBunditResponse = class("OpenOnearmBunditResponse")

function OpenOnearmBunditResponse:ctor()
	--响应消息号
	self.order = 30001
	--返回结果,1:成功申请
	self.result =  ""
	--玩老虎机的次数
	self.param1 =  ""
	--物品信息
	self.a_param1 =  ""	
end

function OpenOnearmBunditResponse:OpenOnearmBunditResponse(data)
	dump(data)
	if data.result == 1 then
		if SlotModel then
			SlotModel:update(data)
		else
			SlotModel = require("app.model.SlotModel").new()
			SlotModel:update(data)
		end
		GameDispatcher:dispatchEvent({name = EVENT_CONSTANT.NET_CALLBACK,data = data})
	end
end

return OpenOnearmBunditResponse